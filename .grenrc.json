{
  "dataSource": "prs",
  "prefix": "",
  "ignoreLabels": [],
  "onlyMilestones": false,
  "groupBy": false,
  "template": {
    "commit": "- [{{message}}]({{url}}) - @{{author}}",
    "issue": "- {{name}} [{{text}}]({{url}})",
    "label": "[**{{label}}**]",
    "noLabel": "closed",
    "group": "\n#### {{heading}}\n",
    "changelogTitle": "# Vitrine Changelog\n\n",
    "release": "## {{release}} ({{date}})\n{{body}}",
    "releaseSeparator": "\n---\n\n"
  },
  "milestoneMatch": "Release {{tag_name}}"
}
